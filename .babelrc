{
  "env": {
     "jest": {
      "presets": ["es2015", "react"],
      "plugins": ["transform-object-rest-spread"]
    },
    "webpack": {
      "presets": ["react", "es2015-rollup"],
      "plugins": ["transform-object-rest-spread", "transform-runtime"]
    },
    "rollup": {
      "presets": ["react", "es2015-rollup"],
      "plugins": ["transform-object-rest-spread"]
    }
  }
}
